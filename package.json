{
  "name": "drjulian-challenge",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/TellUsSoftware/drjulian-challenge"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.0.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development npm run serve",
    "watch:dev": "cross-env NODE_ENV=development npm run watch",
    "build": "npm run clean-dist && npm run build-ts && npm run lint",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch-test": "npm run test -- --watchAll",
    "serve": "npm run build && node dist/server.js",
    "serve-debug": "npm run build && nodemon --inspect dist/server.js",
    "watch": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"nodemon dist/server.js\"",
    "watch-debug": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"nodemon --inspect dist/server.js\"",
    "inspect": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"nodemon --inspect dist/server.js\"",
    "clean-dist": "rimraf dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "aws-sdk": "^2.964.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config-node": "^1.3.0",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.5",
    "cron": "^1.8.2",
    "domhandler": "^4.2.0",
    "email-templates": "^8.0.7",
    "errorhandler": "^1.5.1",
    "exceljs": "^4.2.1",
    "express": "^4.17.1",
    "fcm-push": "^1.1.3",
    "gm": "^1.23.1",
    "handlebars": "^4.7.6",
    "helmet": "^4.6.0",
    "html-docx-js": "^0.3.1",
    "html-entities": "^2.3.2",
    "html-pdf-node": "^1.0.7",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "method-override": "^3.0.0",
    "mime": "^2.5.0",
    "moment": "^2.29.1",
    "moment-round": "git://github.com/fvinogradov/moment-round.git#master",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "pg-boss": "^6.1.3",
    "q": "^1.4.1",
    "qs": "^6.10.1",
    "request": "^2.88.2",
    "response-time": "^2.3.2",
    "sanitize-html": "^2.4.0",
    "sequelize": "^6.6.5",
    "socket.io": "^4.1.3",
    "stripe": "^8.168.0",
    "swagger-jsdoc": "^6.1.0",
    "twilio": "^3.66.1",
    "umzug": "^2.3.0",
    "uuid": "^8.3.2",
    "ya-csv": "^1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.2.21",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.3",
    "@types/email-templates": "^8.0.4",
    "@types/errorhandler": "^1.5.0",
